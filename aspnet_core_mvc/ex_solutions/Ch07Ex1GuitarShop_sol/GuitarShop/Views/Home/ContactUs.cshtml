@model Dictionary<string, string>

@{
    ViewData["Title"] = "Contact Us";
}
<h1>Contact Us</h1>

<p>Here's how you can reach us:</p>

<table class="table table-bordered table-striped">
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.Key</td>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>

